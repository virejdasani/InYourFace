{
  "name": "in-your-face",
  "displayName": "In Your Face",
  "description": "In Your Face, shows you Doom 'Ouch Faces' that correlate to the number of errors in your code!",
  "version": "1.1.2",
  "publisher": "VirejDasani",
  "homepage": "https://github.com/virejdasani/InYourFace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/virejdasani/InYourFace"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "keywords": [
    "in your face",
    "visualization",
    "doom",
    "ouch",
    "face",
    "errors",
    "error lens",
    "error count"
  ],
  "__metadata": {
    "publisherDisplayName": "Virej Dasani"
  },
  "activationEvents": [
    "onCommand:in-your-face.openview.focus",
    "onView:in-your-face.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
        "id": "custom-activitybar",
        "title": "In Your Face",
        "icon": "assets/logo_bito.svg"
      }]
    },
    "views": {
      "custom-activitybar": [{
        "type": "webview",
        "id": "in-your-face.openview",
        "name": "In Your Face",
        "contextualTitle": "In Your Face"
      }]
    },
    "configuration": {
      "title": "InYourFace",
      "properties": {
          "InYourFace.error.usewarnings": {
              "type": "boolean",
              "default": false,
              "description": "Should take in account warnings"
          },
          "InYourFace.face.useDoomFace":{
            "type": "boolean",
              "default": true,
              "description": "Uses the DOOM FACEðŸ”¥"
          }
      }
    },
    "commands": [{
      "command": "in-your-face.openview.focus",
      "category": "In Your Face",
      "title": "Open Sidebar",
      "icon": "$(clear-all)"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  }
}